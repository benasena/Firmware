#!nsh
#
# @name Hexarotor Reverse Y6B VTOL Tailsitter
#
# @type VTOL Hexa Cox (Backwards) Tailsitter
# @class VTOL
#
# @maintainer Ben Sena <bs@mit.edu>
#

sh /etc/init.d/rc.vtol_defaults

if [ $AUTOCNF == yes ]
then
    param set VT_MOT_COUNT 6
    param set VT_IDLE_PWM_MC  1080
    param set VT_TYPE 20
#   param set VT_TYPE 25
    param set VT_ELEV_MC_LOCK 1
    param set VT_FW_DIFTHR_EN 1
    param set VT_DIFTHR_SC 1.0
    
fi

set MIXER hexa_cox_bax_vtol

set MAV_TYPE 20
